<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- ListView的样式 -->
    <style name="NormalListViewStyle">
        <item name="android:layout_margin">@dimen/base8dp</item>
        <item name="android:divider">@color/list_div</item>
        <item name="android:dividerHeight">@dimen/base8dp</item>
        <item name="android:fadingEdge">none</item>
        <item name="android:scrollbars">none</item>
    </style>

    <!-- 编辑EditText的样式 -->
    <style name="NormalEditTextStyle">
        <item name="android:gravity">center</item>
        <item name="android:background">@null</item>
        <item name="android:paddingRight">@dimen/base8dp</item>
        <item name="android:textColor">#88000000</item>
        <item name="android:textSize">16sp</item>
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
    </style>

    <!-- 显示详情的TextView标题的样式 -->
    <style name="NormalMessageNameStyle">

        <item name="android:layout_gravity">left</item>
        <item name="android:textColor">@color/message_name</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_width">@dimen/base78dp</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <!-- 显示详情的TextView内容的样式 -->
    <style name="NormalContent1Style">
        <item name="android:background">@null</item>
        <item name="android:layout_gravity">left|center</item>
        <item name="android:textColor">@color/message_content</item>
        <item name="android:textSize">16sp</item>
        <!--<item name="android:width">0dp</item>-->
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">4</item>
        <item name="android:layout_marginLeft">@dimen/base4dp</item>
    </style>

    <!--编辑信息的EditText的样式  -->
    <style name="NormalEditText2Style">
        <item name="android:background">#00000000</item>
        <item name="android:gravity">left</item>
        <item name="android:singleLine">false</item>
        <item name="android:inputType">textMultiLine</item>
        <item name="android:textColor">@color/message_content</item>
        <item name="android:textSize">15sp</item>

    </style>

    <!--底部长按钮的样式抽取 -->
    <style name="NormalBigBottomButtonStyle">
        <item name="android:background">@drawable/btn_bg</item>
        <item name="android:layout_height">@dimen/base44dp</item>
        <item name="android:layout_width">@dimen/base320dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/base18dp</item>
        <item name="android:layout_marginTop">@dimen/base30dp</item>
        <item name="android:layout_marginBottom">@dimen/base30dp</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:text">提交</item>
    </style>

