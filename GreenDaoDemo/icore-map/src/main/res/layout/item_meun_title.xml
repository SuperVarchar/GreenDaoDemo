<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:paddingLeft="@dimen/base15dp"
    android:paddingRight="@dimen/base15dp"
    android:paddingTop="@dimen/base10dp"
    android:paddingBottom="@dimen/base10dp"
    >
    <ImageView
        android:src="@drawable/wjg_add"
        android:layout_gravity="center_vertical"
        android:id="@+id/meun_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
       />
    <TextView
        android:text="13456"
        android:layout_gravity="center_vertical"
        android:layout_marginLeft="@dimen/base10dp"
        android:id="@+id/meun_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/white"
        android:textSize="@dimen/base13dp"/>
</LinearLayout>